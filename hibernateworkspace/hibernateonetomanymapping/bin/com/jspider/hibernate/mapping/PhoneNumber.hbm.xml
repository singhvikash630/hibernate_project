<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.8//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jspider.hibernate.beans.PhoneNumber" table="contact_numbers">

		<id name="id" type="java.lang.Integer" column="id" />
		<property name="numbertype" type="java.lang.String" column="numbertype" />
		<property name="number" type="java.lang.Integer" column="number" />

	</class>
</hibernate-mapping>